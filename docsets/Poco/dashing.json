{
    "name": "Poco",
    "package": "poco",
    "index":"index.html",
    "icon32x32": "poco.png",
    "externalURL": "https://pocoproject.org/",
    "selectors": {
        ".symbol-namespace": {
            "type": "Namespace",
            "regexp": "namespace ",
            "replacement": "",
            "matchpath": "Poco.*\\.html"
        },
        ".symbol-class": {
            "type": "Class",
            "regexp": "class ",
            "replacement": "",
            "matchpath": "Poco.*\\.html"
        },
        ".symbol-struct": {
            "type": "Struct",
            "regexp": "struct ",
            "replacement": "",
            "matchpath": "Poco.*\\.html"
        },
        ".symbol-functions": {
            "type": "Function"
        },
        ".symbol-constructors": {
            "type": "Constructor"
        },
        ".symbol-enums": {
            "type": "Enumerations",
            "matchpath": "Poco.*\\.html"
        }
    },
    "ignore": [
        "ABOUT"
    ]
}