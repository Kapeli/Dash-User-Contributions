{
    "name": "GraphQL June 2018 Edition",
    "package": "graphql",
    "index": "index.html",
    "selectors": {
        "h2": {
          "type": "Guide",
          "regexp": "([A-Z0-9.][0-9.]*)(.*)",
          "replacement": "$1 $2"
        },
        "h3": {
          "type": "Section",
          "regexp": "([A-Z0-9.][0-9.]*)(.*)",
          "replacement": "$1 $2"
        }
    },
    "icon32x32": "icon.png"
}
