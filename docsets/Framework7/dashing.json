{
    "name": "Framework7",
    "package": "Framework7",
    "index": "html/framework7.io/docs/index.html",
    "selectors": {
        "h1": [
            {
                "type": "Guide",
                "regexp": "(.*)",
                "replacement": "API $1",
                "matchpath": "html/framework7\\.io/docs/.*\\.html"
            },
            {
                "type": "Guide",
                "regexp": "(.*)",
                "replacement": "React $1",
                "matchpath": "html/framework7\\.io/react/.*\\.html"
            },
            {
                "type": "Guide",
                "regexp": "(.*)",
                "replacement": "Vue $1",
                "matchpath": "html/framework7\\.io/vue/.*\\.html"
            },
            {
                "type": "Plugin",
                "matchpath": "html/framework7\\.io/docs/plugins-api\\.html"
            }
        ],
        "h2": "Section"
    },
    "icon32x32": "icon.png",
    "allowJS": true,
    "ExternalURL": "http://framework7.io/docs/"
}
