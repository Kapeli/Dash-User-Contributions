{
    "name": "SWI Prolog",
    "package": "swipl",
    "index": "index.html",
    "selectors": {
        "h2,h3,h4 > a > span.sec-title:nth-child(2)": {
            "requiretext": "library\\(.+\\): ",
            "type":"Library",
            "regexp": "^([A-Z])*[0-9. ]*|\\s\\n",
            "replacement": ""
        },
        "dl.latex dt.pubdef a": {
          "type":"Procedure",
          "regexp": "\\s\\n",
          "replacement": " "
        }
    },
    "ignore": [
        "ABOUT Acknowledgments Acknowledgements"
    ],
    "icon32x32": "favicon.ico",
    "allowJS": false,
    "ExternalURL": "http://www.swi-prolog.org/pldoc"
}
